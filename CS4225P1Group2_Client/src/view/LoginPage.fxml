<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginPage">
   <children>
      <Text fx:id="errorText" fill="RED" layoutX="126.0" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username Cannot Be Blank" visible="false" wrappingWidth="149.0" />
      <TextField fx:id="usernameTextField" layoutX="109.0" layoutY="195.0" prefHeight="33.0" prefWidth="185.0" />
      <Text fx:id="welcomeText" layoutX="91.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Text>
      <Text fx:id="createUsernameText" layoutX="125.0" layoutY="167.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create a Username">
         <font>
            <Font size="18.0" />
         </font></Text>
      <Button fx:id="loginButton" layoutX="156.0" layoutY="241.0" mnemonicParsing="false" onMouseClicked="#handleLogin" prefHeight="33.0" prefWidth="92.0" text="Login" />
      <Text fx:id="userNameTakenText" fill="RED" layoutX="76.0" layoutY="338.0" strokeType="OUTSIDE" strokeWidth="0.0" text="That user name has been taken" visible="false">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
