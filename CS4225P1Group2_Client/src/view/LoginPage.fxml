         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Text>
      <Text fx:id="createUsernameText" layoutX="150.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create a Username" />
      <Button fx:id="loginButton" layoutX="175.0" layoutY="217.0" mnemonicParsing="false" onMouseClicked="#handleLogin" text="Login" />
